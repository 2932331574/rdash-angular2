{
  "name": "rdash-angular2",
  "version": "0.0.2",
  "description": "AngularJS2 implementation of the RDash admin dashboard theme http://rdash.github.io",
  "repository": "https://github.com/ziyasal/rdash-angular2",
  "scripts": {
    "postinstall": "npm run typings && npm run bower",
    "tsc": "tsc",
    "tsc:w": "tsc --watch",
    "live": "live-server --open=src",
    "start": "concurrently \"npm run tsc:w\" \"npm run live\" ",
    "typings": "typings install",
    "bower": "bower install"
  },
  "author": "Ziya SARIKAYA <ziyasal>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.0.0",
    "live-server": "^0.9.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.14",
    "systemjs": "0.19.25",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  }
}
